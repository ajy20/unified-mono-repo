{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{png,jpg,gif,svg}", "/css/*"]
  },
  "routes": [
    {
      "route": "/remoteEntry.json",
      "headers": {
        "cache-control": "no-store, no-cache, must-revalidate, max-age=0"
      }
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*"
  }
}