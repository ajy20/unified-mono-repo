{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/products/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,json}"]
  },
  "routes": [
    {
      "route": "/products/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript"
  }
}