{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/products/*",
      "/*.{css,scss,js,png,gif,ico,jpg,jpeg,svg,json,woff,woff2,ttf,eot}"
    ]
  },
  "routes": [
    {
      "route": "/products/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS"
  }
}